{{!--
{
  title: 'Results of last BreakOut',
  vars: {
    teamsheading: {
      title: 'Heading for teams',
      type: 'text'
    },
    teamstext: {
      title: 'Text for teams',
      type: 'text'
    },
    donationsheading: {
      title: 'Heading for donations',
      type: 'text'
    },
    donationstext: {
      title: 'Text for donations',
      type: 'text'
    },
    distanceheading: {
      title: 'Heading for distance covered',
      type: 'text'
    },
    distancetext: {
      title: 'Text for distance covered',
      type: 'text'
    },
  }
