{
  "name": "breakout-frontend",
  "version": "1.0.0",
  "description": "Web Frontend",
  "main": "app.js",
  "scripts": {
    "postinstall": "npm run build",
    "prestart": "npm run build",
    "build": "grunt",
    "start": "node app.js",
    "test": "mocha tests/",
    "log:info": "cd logs && cat info.log | npm run bunyan -s",
    "log:error": "cd logs && cat error.log | npm run bunyan -s",
    "log:clean": "rm logs/*.log",
    "log:live_info": "tail -f logs/info.log | npm run bunyan -s",
    "mocha": "mocha",
    "grunt": "grunt",
    "bunyan": "bunyan --color",
    "wdio": "wdio"
  },
