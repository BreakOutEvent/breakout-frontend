{
  "name": "breakout-frontend",
  "version": "1.0.0",
  "description": "Web Frontend",
  "main": "index.html",
  "scripts": {
    "postinstall": "npm run build && cd src/cms && npm install",
    "prestart": "npm run build && npm run cms:build",
    "build": "grunt",
    "start": "node app.js",
    "cms:build": "cd src/cms && npm run build",
    "cms:update": "cd src/cms && npm update && npm run build",
    "cms:server": "cd src/cms && npm run server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "grunt": "grunt",
    "bunyan": "bunyan",
    "rimraf": "rimraf",
    "webpack": "webpack",
    "log:info": "cd logs && cat info.log | npm run bunyan",
    "log:error": "cd logs && cat error.log | npm run bunyan",
    "log:clean": "rm logs/*.log",
    "log:live_info": "tail -f logs/info.log | npm run bunyan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BreakOutEvent/breakout-frontend.git"
  },
  "author": "BreakOut <admin@break-out.org>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/BreakOutEvent/breakout-frontend/issues"
  },
  "homepage": "https://github.com/BreakOutEvent/breakout-frontend#readme",
  "dependencies": {
    "bluebird": "^3.3.4",
    "body-parser": "^1.14.2",
    "bootstrap-sass": "^3.3.6",
    "bunyan": "^1.8.0",
    "co": "^4.6.0",
    "co-fs-extra": "^1.2.1",
    "co-request": "^1.0.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "express-session": "^1.13.0",
    "google-spreadsheet": "^2.0.1",
    "jquery": "^2.2.0",
    "lodash": "^4.8.2",
    "mongoose": "^4.3.3",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "passport": "^0.2.2",
    "passport-local": "^1.0.0",
    "request": "^2.67.0",
    "throng": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "grunt": "^0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^4.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-uglify": "^0.11.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-jsbeautifier": "^0.2.10",
    "grunt-sass": "^1.1.0",
    "path": "^0.12.7",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14"
  },
  "jscsConfig": {
    "validateLineBreaks": false,
    "requireTrailingComma": false,
    "requireSpacesInAnonymousFunctionExpression": false
  }
}
